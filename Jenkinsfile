pipeline{
  agent any
 stages{
  stages('checkout'){
    steps{
      echo'cloning the repo into local server'
      }
     } 
   stages('build'){
    steps{
     echo'build the code into package'
      } 
     }
   stages('test'){
    steps{
     echo'Testing the application'
      } 
     }
   stages('deploy'){
    steps{
     echo'Hello World'
      }
     }
    }
   }  
